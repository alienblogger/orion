{
  "name": "orion",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "axios": "*",
    "lodash": "^4.17.15",
    "nosleep.js": "^0.9.0",
    "riot": "^3.7.0",
    "toastify-js": "^1.6.1"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "concurrently": "*",
    "riotify": "^2.0.0",
    "serve": "*",
    "watchify": "^3.9.0",
    "yarn": "^1.22.0"
  },
  "scripts": {
    "prestart": "npm i",
    "watch": "watchify app.js -t riotify -o app.bundle.js -v",
    "build": "sh build.sh",
    "browserify": "browserify app.js -t riotify -o public/app.bundle.js",
    "riot:watch": "riot -m -w src public/tags.js",
    "compile": "riot -m src tags.js",
    "serve": "serve .",
    "start": "concurrently 'yarn serve' 'yarn riot:watch' 'yarn watch'"
  }
}